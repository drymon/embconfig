
//Nested data structure

struct ABC abc; //Global variable export to the user application

struct MNP { //Struct definition
	int m;
	int n;
	int p;
};

struct XYZ { //Struct definition
	float x;
	double y;
	int z;
	struct MNP mnp;
};

struct ABC { //Struct definition
	int a;
	float b;
	int c;
	struct XYZ xyz;
	struct MNP abc_mnp;
};
